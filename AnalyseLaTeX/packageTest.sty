% les sections alternatives
%%%%%%
\newcommand{\NouvelleSubsection}[1]{
  \newpage
  \subsection{#1}
  \stepcounter{Subsection}
}
\newcommand{\PremiereNouvelleSubsection}[1]{
    \subsection{#1}
    \stepcounter{Subsection}
}
\newcommand{\NouvellePart}[1]{
  \newpage
  \part{#1}
  \setcounter{Subsection}{1}
  \setcounter{prop}{0}
}


